{
  "version": 2,
  "builds": [
      {
          "src": "/(.*)",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/api",
          "methods": [
              "GET",
              "POST",
              "PUT",
              "DELETE",
              "OPTIONS"
          ]
      }
  ] 
}